{
	// TODO Auto-generated constructor stub

}

void WobbleBoardApp::Init()
{
	InitInertialSensors();
}

void WobbleBoardApp::InitInertialSensors()
{
	BSP_SENSOR_ACC_Init();
	BSP_SENSOR_GYR_Init();
	BSP_SENSOR_MAG_Init();
}

